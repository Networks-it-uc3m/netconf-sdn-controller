<ipsec-ikeless
  xmlns="urn:ietf:params:xml:ns:yang:ietf-i2nsf-ikeless"
  xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
  <spd>
	<spd-entry>
		<!-- <rule-number>11</rule-number> -->
		<rule-number>11</rule-number>
		<priority>0</priority>
		<names>
			<name>out/192.168.201.254/192.168.202.254</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>2</ts-number>
				<direction>OUTBOUND</direction>
				<local-addresses>
					<start>192.168.201.0/24</start>
 					<end>192.168.201.0/24</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.202.0/24</start>
					<end>192.168.202.0/24</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TUNNEL</mode>
        		<tunnel>
          			<local>192.168.123.100</local>
          			<remote>192.168.123.200</remote>
        		</tunnel>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
  </spd>
</ipsec-ikeless>
